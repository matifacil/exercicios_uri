#include <iostream>
#include <iomanip>

using namespace std;
int main() {
	int a,b,c;
	while(cin >>a>>b>>c){
      if (a>b && b>c && a>c){
         cout<<c<<endl<<b<<endl<<a<<endl;
      }else if(a>b && b<c && a>c){
          cout<<b<<endl<<c<<endl<<a<<endl;
      }else if(a<b && b>c && a>c){
          cout<<c<<endl<<a<<endl<<b<<endl;
      }else if (b>c && c>a && b>a){
         cout<<a<<endl<<c<<endl<<b<<endl;
      }else if(c>a && a>b && c>b){
         cout<<b<<endl<<a<<endl<<c<<endl;
      }else if(c>b && b>a && c>a){
         cout<<a<<endl<<b<<endl<<c<<endl;
      }
      cout<<endl;
      cout<<a<<endl;
      cout<<b<<endl;
      cout<<c<<endl;
	}
  return 0;
}


